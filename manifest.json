{
    "name": "New Tab Page",
    "version": "1.0.02",
    "manifest_version": 2,
    "description": "A simple new tab page extension.",
    "chrome_url_overrides": {
        "newtab": "newTab.html"
    },
    "author":"zhuwenq",
    "browser_action":{
        "default_icon":"/icon/main-icon.png",
        "default_title":"Have a good day!"
    },
    "permissions":[
        "storage"
    ],
    "content_security_policy": "script-src 'self' 'unsafe-eval'"
}